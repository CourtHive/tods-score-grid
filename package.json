    "lint:staged": "lint-staged",
    "prebuild": "rm -rf dist/*",
    "prepare": "husky install",
    "prerelease": "vite build",
    "build": "vite build",
    "release": "np --no-tests",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CourtHive/tods-score-grid.git"
  },
